{
  "name": "frontend",
  "private": true,
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
