{
  "name": "@mizpos/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./accounts": {
      "types": "./dist/generated/accounts.d.ts",
      "import": "./dist/generated/accounts.js"
    },
    "./stock": {
      "types": "./dist/generated/stock.d.ts",
      "import": "./dist/generated/stock.js"
    },
    "./sales": {
      "types": "./dist/generated/sales.d.ts",
      "import": "./dist/generated/sales.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "lint": "biome check .",
    "fix": "biome check --write .",
    "generate": "openapi-typescript --redocly ./redocly.yaml",
    "generate:prod": "openapi-typescript --redocly ./redocly.prod.yaml"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.0"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1"
  }
}
